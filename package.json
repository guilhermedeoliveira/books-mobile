{
  "name": "BooksMobile",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "lint": "eslint ./src/* --ignore-pattern .gitignore",
    "fix": "eslint ./src/* --fix --no-ignore",
    "ios:dev": "ENVFILE=.env.development react-native run-ios",
    "ios:prod": "ENVFILE=.env.production react-native run-ios",
    "android:dev": "ENVFILE=.env.development react-native run-android",
    "android:prod": "ENVFILE=.env.production react-native run-android",
    "android-build:dev": "cd android && ENVFILE=.env.development ./gradlew assembleRelease && cd ..",
    "android-build:prod": "cd android && ENVFILE=.env.production ./gradlew assembleRelease && cd .."
  },
  "dependencies": {
    "react": "16.6.1",
    "react-native": "0.57.5",
    "react-navigation": "^3.0.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "babel-jest": "23.6.0",
    "babel-preset-react-native": "^4.0.1",
    "eslint": "^5.9.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-import": "^0.13.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.11.1",
    "jest": "23.6.0",
    "metro-react-native-babel-preset": "0.49.2",
    "prettier": "^1.15.2",
    "react-native-version": "^2.6.6",
    "react-test-renderer": "16.6.1"
  },
  "jest": {
    "preset": "react-native"
  }
}
